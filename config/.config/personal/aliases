#!/usr/bin/env zsh

alias zfg="zed ~/.zshrc"
alias zprofile="time ZSH_DEBUGRC=1 zsh -i -c exit"
alias dc="docker compose"
alias cat="bat"
alias gfg="zed $XDG_CONFIG_HOME/ghostty/config"
alias mcp="nvim /Users/gunnarholwerda/Library/Application\ Support/Claude/claude_desktop_config.json"
alias claude="claude --dangerously-skip-permissions"


function cdg() {
    local dir
    dir=$(tv git-repos) && cd "$dir"
}

